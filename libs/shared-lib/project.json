{
  "name": "@r360/shared-lib",
  "targets": {
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest run",
        "cwd": "{projectRoot}"
      },
      "configurations": {
        "coverage": {
          "command": "vitest run --coverage"
        }
      },
      "outputs": ["{projectRoot}/test-output/vitest/coverage"],
      "cache": true
    }
  }
}
